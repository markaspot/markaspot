{
  "name": "drupal/markaspot",
  "description": "Installation profile of the Mark-a-Spot D8 Distribution",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/markaspot/markaspot.git",
  "authors": [
    {
      "name": "Holger Kreis (markaspot)",
      "homepage": "https://www.drupal.org/u/markaspot",
      "role": "Maintainer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "npm-asset/leaflet.locatecontrol": "^0.65",
    "npm-asset/fontawesome-iconpicker": "^1.3",
    "npm-asset/waypoints": "4.0.1",
    "npm-asset/iso8601-js-period": "^0.2",
    "npm-asset/dateformat": "3.0.2",
    "npm-asset/lazysizes": "^5.1",
    "npm-asset/leaflet": "~1",
    "npm-asset/leaflet-timedimension": "^1",
    "npm-asset/leaflet.heat": "0.2.0",
    "npm-asset/leaflet.markercluster": "^1.4",
    "npm-asset/leaflet-fullscreen": "^1",
    "npm-asset/leaflet-edgebuffer": "^1",
    "npm-asset/leaflet-easybutton": "2.4.0",
    "npm-asset/leaflet-control-geocoder": "^1",
    "npm-asset/drmonty-leaflet-awesome-markers": "2.0.2",
    "npm-asset/mapbox-gl-leaflet": "^0.0.15",
    "npm-asset/mapbox-gl": "*",
    "npm-asset/font-awesome": "^5",
    "npm-asset/axios": "^0.18.0",
    "npm-asset/caman": "^4.1",
    "npm-asset/chart.js": "^2.7",
    "npm-asset/vue": "^2.5",
    "npm-asset/vue-chartjs": "^3.4",
    "npm-asset/vue-router": "^3.0",
    "oomphinc/composer-installers-extender": "^2",
    "composer/installers": "^1.2",
    "funiq/geophp": "dev-master",
    "anthonymartin/geo-location": "^1.0",
    "drupal/address": "~1.0",
    "drupal/features": "^3.0",
    "drupal/pathauto": "^1",
    "drupal/admin_toolbar": "^3",
    "drupal/advagg": "^4.1",
    "drupal/restui": "^1",
    "drupal/token": "^1.9",
    "drupal/better_normalizers": "1.x-dev",
    "drupal/ctools": "3.x-dev",
    "drupal/image_effects": "^3",
    "drupal/layout_library": "^1.0@beta",
    "drupal/lazy": "^3.0",
    "drupal/paragraphs": "^1.2",
    "drupal/entity_reference_revisions": "^1.9",
    "drupal/diff": "^1.0",
    "drupal/exif_orientation": "^1",
    "drupal/field_group": "^3.1",
    "drupal/form_mode_manager": "^2",
    "drupal/geolocation": "^3",
    "drupal/maillog": "1.x-dev",
    "drupal/media_parent_entity_link": "^1.0",
    "drupal/metatag": "^1.16",
    "drupal/services_api_key_auth": "dev-3.0.x",
    "drupal/search_api": "^1.5",
    "drupal/views_infinite_scroll": "^1.5",
    "drupal/better_exposed_filters": "^5.0",
    "drupal/default_content": "2.0.x",
    "drupal/color_field": "^2.4",
    "drupal/field_permissions": "^1.1",
    "drupal/twig_tweak": "^2.0",
    "drupal/field_formatter_class":"^1.0",
    "drupal/pbf": "^1.3",
    "drupal/inline_entity_form": "^1.0@RC"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "enable-patching": true,
    "patches": {
      "drupal/business_rules": {
        "Add taxonomy condition": "https://www.drupal.org/files/issues/2018-07-05/2983864-feature-request-compare-target_id-with-field.patch"
      },
      "drupal/core": {
        "Add UI for field formatter translation": "https://www.drupal.org/files/issues/2020-12-04/2546212-168.patch",
        "Add UI for field formatter translation / Action tab": "https://www.drupal.org/files/issues/2020-12-04/2546212-168-actions.patch"
      }
    }
  }
}
