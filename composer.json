{
  "name": "drupal/markaspot",
  "description": "Installation profile of the Mark-a-Spot D8 Distribution",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/markaspot/markaspot.git",
  "authors": [
    {
      "name": "Holger Kreis (markaspot)",
      "homepage": "https://www.drupal.org/u/markaspot",
      "role": "Maintainer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/markaspot/GeoPHP"
    }
  ],
  "require": {
    "bower-asset/bootstrap-select": "1.12.4",
    "npm-asset/leaflet.locatecontrol": "0.62.0",
    "npm-asset/leaflet-control-geocoder": "1.5.6",
    "npm-asset/fontawesome-iconpicker": "^1.3",
    "npm-asset/waypoints": "4.0.1",
    "npm-asset/iso8601-js-period": "0.2.0",
    "npm-asset/dateformat": "3.0.2",
    "npm-asset/leaflet": "~1.0.0",
    "npm-asset/leaflet-timedimension": "^1.0",
    "npm-asset/leaflet.heat": "0.2.0",
    "npm-asset/leaflet.markercluster": "1.2.0",
    "npm-asset/leaflet-fullscreen": "1.0.2",
    "npm-asset/leaflet-easybutton": "2.2.2",
    "npm-asset/drmonty-leaflet-awesome-markers": "2.0.2",
    "npm-asset/font-awesome": "4.7.0",
    "npm-asset/pace-js": "^0.7",
    "npm-asset/shariff": "2.0.2",
    "bower-asset/slideout.js": "1.0.1",
    "oomphinc/composer-installers-extender": "^1.1",
    "composer/installers": "^1.2",
    "drupal-composer/drupal-scaffold": "^2.2",
    "cweagans/composer-patches": "~1.6",
    "phayes/geophp": "dev-master",
    "anthonymartin/geo-location": "^1.0",
    "drush/drush": "~9.0",
    "drupal/address": "~1.0",
    "drupal/features": "^3.0",
    "drupal/core": "8.6.x-dev",
    "drupal/radix": "3.*",
    "drupal/pathauto": "^1.0@RC",
    "drupal/admin_toolbar": "1.*",
    "drupal/restui": "^1.15",
    "drupal/token": "1.*",
    "drupal/shariff": "dev-1.x",
    "drupal/r4032login": "dev-1.x",
    "drupal/bootstrap_layouts": "^5.0",
    "drupal/views_bootstrap": "^3.0",
    "drupal/better_normalizers": "1.x-dev",
    "drupal/ctools": "3.x-dev",
    "drupal/image_effects": "1.x-dev",
    "drupal/paragraphs": "^1.2",
    "drupal/entity_reference_revisions": "^1.3",
    "drupal/diff": "^1.0",
    "drupal/field_group": "^1.0",
    "drupal/form_mode_manager": "~2",
    "drupal/geolocation": "^1.11",
    "drupal/rules": "3.x-dev",
    "drupal/maillog": "1.x-dev",
    "drupal/services_api_key_auth": "1.*",
    "drupal/search_api": "^1.5",
    "drupal/views_infinite_scroll": "^1.5",
    "drupal/better_exposed_filters": "^3.0",
    "drupal/default_content": "1.x-dev",
    "drupal/color_field": "2.x-dev",
    "drupal/field_permissions": "1.x-dev",
    "drupal/twig_tweak": "^2.0",
    "drupal/field_formatter_class":"^1.0",
    "drupal/hide_revision_field": "^1.1",
    "drupal/page_manager": "4.x-dev",
    "drupal/panels": "^4.2",
    "drupal/entity_clone": "^1.0@alpha",
    "drupal/pbf": "^1.1",
    "drupal/quick_node_clone": "^1.5",
    "drupal/tac_lite": "^1.4",
    "drupal/views_bulk_edit": "^2",
    "drupal/views_bulk_operations": "^2",
    "drupal/devel": "^1.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-install-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ],
    "post-update-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ]
  },
  "extra": {
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "enable-patching": true,
    "patches": {
      "drupal/color_field": {
        "Palette instead of default_colors": "https://www.drupal.org/files/issues/default_colors_box-2807783-1.patch"
      },
      "drupal/rules": {
        "Action send mail, implode error": "https://www.drupal.org/files/issues/system-send-email-action-fails-if-to-is-a-string-2827183-2.patch",
        "Send mail text field 2 textarea": "https://www.drupal.org/files/issues/rules_sendmail_textarea-2724129-34.patch",
        "Set new revision to false on entity save": "https://gist.githubusercontent.com/markaspot/049453ad442cea2df105b4b9b74bc331/raw/ae409ed4a23899f33911635c77e52e76960570f9/save%2520entity%2520no%2520revisions"
      },
      "drupal/shariff": {
        "Use dist path for shariff library": "https://gist.githubusercontent.com/markaspot/e36e7f6a21f47a984711c6d5ca40a543/raw/965f199c1d22623771d049f7e94f3a7105e1c45f/shariff%2520drupal%2520dist%2520path"
      }
    }
  }
}
