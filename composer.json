{
  "name": "drupal/markaspot",
  "description": "Installation profile of the Mark-a-Spot D8 Distribution",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/markaspot/markaspot.git",
  "authors": [
    {
      "name": "Holger Kreis (markaspot)",
      "homepage": "https://www.drupal.org/u/markaspot",
      "role": "Maintainer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "npm-asset/bootstrap-select": "1.12",
    "npm-asset/leaflet.locatecontrol": "^0.65",
    "npm-asset/fontawesome-iconpicker": "^1.3",
    "npm-asset/waypoints": "4.0.1",
    "npm-asset/iso8601-js-period": "0.2.0",
    "npm-asset/dateformat": "3.0.2",
    "npm-asset/leaflet": "~1",
    "npm-asset/leaflet-timedimension": "^1.1",
    "npm-asset/leaflet.heat": "0.2.0",
    "npm-asset/leaflet.markercluster": "^1.4",
    "npm-asset/leaflet-fullscreen": "1.0.2",
    "npm-asset/leaflet-easybutton": "2.4.0",
    "npm-asset/leaflet-control-geocoder": "^1",
    "npm-asset/drmonty-leaflet-awesome-markers": "2.0.2",
    "npm-asset/font-awesome": "4.7.0",
    "npm-asset/pace-js": "^1",
    "npm-asset/shariff": "2.1.1",
    "npm-asset/axios": "^0.18.0",
    "npm-asset/caman": "^4.1",
    "npm-asset/chart.js": "^2.7",
    "npm-asset/vue": "^2.5",
    "npm-asset/vue-chartjs": "^3.4",
    "npm-asset/vue-router": "^3.0",
    "bower-asset/bootstrap-offcanvas": "^2.5",
    "oomphinc/composer-installers-extender": "^2",
    "composer/installers": "^1.2",
    "funiq/geophp": "dev-master",
    "anthonymartin/geo-location": "^1.0",
    "drupal/address": "~1.0",
    "drupal/features": "^3.0",
    "drupal/radix": "3.x-dev",
    "drupal/pathauto": "^1",
    "drupal/admin_toolbar": "^3",
    "drupal/restui": "^1",
    "drupal/token": "^1.9",
    "drupal/shariff": "^1",
    "drupal/r4032login": "^2",
    "drupal/bootstrap_layouts": "^5.0",
    "drupal/views_bootstrap": "^3.0",
    "drupal/better_normalizers": "1.x-dev",
    "drupal/ctools": "3.x-dev",
    "drupal/image_effects": "^3",
    "drupal/paragraphs": "^1.2",
    "drupal/entity_reference_revisions": "^1.9",
    "drupal/diff": "^1.0",
    "drupal/exif_orientation": "^1",
    "drupal/field_group": "^3.1",
    "drupal/form_mode_manager": "2.x-dev",
    "drupal/geolocation": "^3",
    "drupal/maillog": "1.x-dev",
    "drupal/services_api_key_auth": "^3",
    "drupal/search_api": "^1.5",
    "drupal/views_infinite_scroll": "^1.5",
    "drupal/better_exposed_filters": "^5.0",
    "drupal/default_content": "2.0.x",
    "drupal/color_field": "^2.4",
    "drupal/field_permissions": "^1.1",
    "drupal/twig_tweak": "^2.0",
    "drupal/field_formatter_class":"^1.0",
    "drupal/page_manager": "4.x-dev",
    "drupal/panels": "^4.2",
    "drupal/pbf": "^1.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "enable-patching": true,
    "patches": {
      "drupal/radix": {
        "Make D9 ready": "web/profiles/contrib/markaspot/patches/radix-d9-3-x.patch"
      },
      "drupal/business_rules": {
        "Add taxonomy condition": "https://www.drupal.org/files/issues/2018-07-05/2983864-feature-request-compare-target_id-with-field.patch",
        "Fix e-mail-from header": "https://www.drupal.org/files/issues/2020-04-28/business-rules-email-from.patch"
      },
      "drupal/form_mode_manager": {
        "Add D9 fixes": "https://www.drupal.org/files/issues/2020-10-30/form_mode_manager-drupal-9-compatibility-3164478-6.patch"
      },
      "drupal/image_effects": {
        "D8.8.6 -> D8.8.7 = Usupport schema version": "https://www.drupal.org/files/issues/2020-07-14/unsuported_schema_version-3146575-7.patch"
      }
    }
  }
}
