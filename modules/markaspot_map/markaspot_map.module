<?php

function markaspot_map_page_attachments(array &$page) {
  $attached = \Drupal::configFactory()
    ->getEditable('markaspot_map.settings')->get();

  $page['#attached']['drupalSettings']['mas'] = $attached;
  $page['#attached']['drupalSettings']['mas'] = $attached;

  $page['#attached']['library'][] = 'markaspot_map/map';


}
